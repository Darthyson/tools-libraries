<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.1" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  targetNamespace="http://knx.org/xml/project/11"
  xmlns="http://knx.org/xml/project/11"
  elementFormDefault="qualified">

  <xs:element name="KNX" type="KnxDocument" />

  <xs:complexType name="KnxDocument">
    <xs:sequence>
      <xs:element name="MasterData" type="MasterData" minOccurs="0" maxOccurs="1" />
      <xs:element name="ManufacturerData" type="ManufacturerData" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
    <xs:attribute name="CreatedBy" type="xs:string" />
    <xs:attribute name="ToolVersion" type="xs:string" />
  </xs:complexType>


  <!--
         Common Types 
    -->

  <xs:complexType name="Identified" abstract="true">
    <xs:attribute name="Id" type="xs:string" use="required" />
  </xs:complexType>

  <xs:complexType name="Reference">
    <xs:attribute name="RefId" type="xs:string" />
  </xs:complexType>

  <xs:complexType name="AbstractProcedure" abstract="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element name="LdCtrlAbsSegment" type="LdCtrlAbsSegment" />
      <xs:element name="LdCtrlClearLCFilterTable" type="LdCtrlClearLCFilterTable" />
      <xs:element name="LdCtrlConnect" type="LdCtrlConnect" />
      <xs:element name="LdCtrlCompareMem" type="LdCtrlCompareMem" />
      <xs:element name="LdCtrlCompareProp" type="LdCtrlCompareProp" />
      <xs:element name="LdCtrlDelay" type="LdCtrlDelay" />
      <xs:element name="LdCtrlDisconnect" type="LdCtrlDisconnect" />
      <xs:element name="LdCtrlLoad" type="LdCtrlLoad" />
      <xs:element name="LdCtrlLoadCompleted" type="LdCtrlLoadCompleted" />
      <xs:element name="LdCtrlLoadImageMem" type="LdCtrlLoadImageMem" />
      <xs:element name="LdCtrlLoadImageProp" type="LdCtrlLoadImageProp" />
      <xs:element name="LdCtrlMapError" type="LdCtrlMapError" />
      <xs:element name="LdCtrlMerge" type="LdCtrlMerge" />
      <xs:element name="LdCtrlRelSegment" type="LdCtrlRelSegment" />
      <xs:element name="LdCtrlRestart" type="LdCtrlRestart" />
      <xs:element name="LdCtrlSetControlVariable" type="LdCtrlSetControlVariable" />
      <xs:element name="LdCtrlTaskPtr" type="LdCtrlTaskPtr" />
      <xs:element name="LdCtrlTaskCtrl1" type="LdCtrlTaskCtrl1" />
      <xs:element name="LdCtrlTaskCtrl2" type="LdCtrlTaskCtrl2" />
      <xs:element name="LdCtrlTaskSegment" type="LdCtrlTaskSegment" />
      <xs:element name="LdCtrlUnload" type="LdCtrlUnload" />
      <xs:element name="LdCtrlWriteMem" type="LdCtrlWriteMem" />
      <xs:element name="LdCtrlWriteProp" type="LdCtrlWriteProp" />
      <xs:element name="LdCtrlWriteRelMem" type="LdCtrlWriteRelMem" />
    </xs:choice>
  </xs:complexType>

  <xs:complexType name="LdCtrlClearLCFilterTable">
    <xs:attribute name="UseFunctionProp" type="xs:boolean" />
  </xs:complexType>

  <xs:complexType name="LdCtrlLoadImageProp">
    <xs:attribute name="ObjIdx" type="xs:integer" use="required" />
    <xs:attribute name="PropId" type="xs:integer" use="required" />
    <xs:attribute name="Count" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlWriteProp">
    <xs:attribute name="ObjType" type="xs:integer" />
    <xs:attribute name="ObjIdx" type="xs:integer" />
    <xs:attribute name="PropId" type="xs:integer" use="required" />
    <xs:attribute name="Count" type="xs:integer" />
    <xs:attribute name="StartElement" type="xs:integer" />
    <xs:attribute name="InlineData" type="xs:hexBinary" />
    <xs:attribute name="Verify" type="xs:boolean" />
  </xs:complexType>

  <xs:complexType name="LdCtrlWriteRelMem">
    <xs:attribute name="ObjIdx" type="xs:integer" use="required" />
    <xs:attribute name="Offset" type="xs:integer" use="required" />
    <xs:attribute name="Size" type="xs:integer" use="required" />
    <xs:attribute name="AppliesTo" type="xs:string" />
    <xs:attribute name="Verify" type="xs:boolean" />
  </xs:complexType>

  <xs:complexType name="LdCtrlRelSegment">
    <xs:attribute name="LsmIdx" type="xs:integer" use="required" />
    <xs:attribute name="Size" type="xs:integer" use="required" />
    <xs:attribute name="Mode" type="xs:integer" use="required" />
    <xs:attribute name="Fill" type="xs:integer" use="required" />
    <xs:attribute name="AppliesTo" type="xs:string" />
  </xs:complexType>

  <xs:complexType name="LdCtrlMapError">
    <xs:attribute name="OriginalError" type="xs:integer" use="required" />
    <xs:attribute name="MappedError" type="xs:integer" use="required" />
  </xs:complexType>

  <xs:complexType name="LdCtrlMerge">
    <xs:attribute name="MergeId" type="xs:integer" use="required" />
  </xs:complexType>

  <xs:complexType name="LdCtrlCompareMem">
    <xs:attribute name="Address" type="xs:integer" use="required" />
    <xs:attribute name="Size" type="xs:integer" use="required" />
    <xs:attribute name="InlineData" type="xs:hexBinary" use="required" />
  </xs:complexType>

  <xs:complexType name="LdCtrlTaskCtrl1">
    <xs:attribute name="LsmIdx" type="xs:integer" use="required" />
    <xs:attribute name="Address" type="xs:integer" />
    <xs:attribute name="Count" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlTaskCtrl2">
    <xs:attribute name="LsmIdx" type="xs:integer" use="required" />
    <xs:attribute name="Callback" type="xs:integer" />
    <xs:attribute name="Address" type="xs:integer" />
    <xs:attribute name="Seg0" type="xs:integer" />
    <xs:attribute name="Seg1" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlSetControlVariable">
    <xs:attribute name="Name" type="xs:string" use="required" />
    <xs:attribute name="Value" type="xs:string" use="required" />
  </xs:complexType>

  <xs:complexType name="LdCtrlWriteMem">
    <xs:attribute name="AddressSpace" type="AddressSpace" />
    <xs:attribute name="Address" type="xs:integer" use="required" />
    <xs:attribute name="Size" type="xs:integer" use="required" />
    <xs:attribute name="Verify" type="xs:boolean" />
    <xs:attribute name="InlineData" type="xs:hexBinary" />
  </xs:complexType>

  <xs:complexType name="LdCtrlLoadImageMem">
    <xs:attribute name="Address" type="xs:integer" use="required" />
    <xs:attribute name="Size" type="xs:integer" use="required" />
  </xs:complexType>

  <xs:complexType name="LdCtrlTaskPtr">
    <xs:attribute name="LsmIdx" type="xs:integer" use="required" />
    <xs:attribute name="InitPtr" type="xs:integer" />
    <xs:attribute name="SavePtr" type="xs:integer" />
    <xs:attribute name="SerialPtr" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlConnect">
  </xs:complexType>

  <xs:complexType name="LdCtrlDisconnect">
  </xs:complexType>

  <xs:complexType name="LdCtrlCompareProp">
    <xs:attribute name="ObjIdx" type="xs:integer" use="required" />
    <xs:attribute name="PropId" type="xs:integer" use="required" />
    <xs:attribute name="InlineData" type="xs:hexBinary" />
  </xs:complexType>

  <xs:complexType name="LdCtrlUnload">
    <xs:attribute name="LsmIdx" type="xs:integer" />
    <xs:attribute name="ObjType" type="xs:integer" />
    <xs:attribute name="Occurrence" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlLoad">
    <xs:attribute name="LsmIdx" type="xs:integer" />
    <xs:attribute name="ObjType" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlAbsSegment">
    <xs:attribute name="LsmIdx" type="xs:integer" use="required" />
    <xs:attribute name="SegType" type="xs:integer" use="required" />
    <xs:attribute name="Address" type="xs:integer" use="required" />
    <xs:attribute name="Size" type="xs:integer" use="required" />
    <xs:attribute name="Access" type="xs:integer" />
    <xs:attribute name="MemType" type="xs:integer" />
    <xs:attribute name="SegFlags" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlTaskSegment">
    <xs:attribute name="LsmIdx" type="xs:integer" use="required" />
    <xs:attribute name="Address" type="xs:integer" use="required" />
  </xs:complexType>

  <xs:complexType name="LdCtrlLoadCompleted">
    <xs:attribute name="LsmIdx" type="xs:integer" />
    <xs:attribute name="ObjType" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="LdCtrlRestart">
  </xs:complexType>

  <xs:complexType name="LdCtrlDelay">
    <xs:attribute name="MilliSeconds" type="xs:integer" use="required" />
  </xs:complexType>


  <!--
         Master Data 
    -->

  <xs:complexType name="MasterData">
    <xs:sequence>
      <xs:element name="DatapointTypes" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="DatapointType" type="DatapointType" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="MediumTypes" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="MediumType" type="MediumType" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="MaskVersions" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="MaskVersion" type="MaskVersion" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Manufacturers" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Manufacturer" type="ManufacturerDetails" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Languages" type="Languages" minOccurs="0" maxOccurs="1" />      
    </xs:sequence>
    <xs:attribute name="Version" type="xs:integer" />
    <xs:attribute name="Signature" type="xs:base64Binary" />
  </xs:complexType>

  <xs:complexType name="DatapointType">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:sequence>
          <xs:element name="DatapointSubtypes" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="DatapointSubtype" type="DatapointSubtype" minOccurs="0" maxOccurs="unbounded" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Number" type="xs:integer" use="required" />
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="Text" type="xs:string" use="required" />
        <xs:attribute name="SizeInBit" type="xs:integer" use="required" />
        <xs:attribute name="Default" type="xs:boolean" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="DatapointSubtype">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:attribute name="Number" type="xs:integer" use="required" />
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="Text" type="xs:string" use="required" />
        <xs:attribute name="Default" type="xs:boolean" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="MediumType">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:attribute name="Number" type="xs:integer" use="required" />
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="Text" type="xs:string" use="required" />
        <xs:attribute name="DomainAddressLength" type="xs:integer" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="MaskVersion">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:sequence>
          <xs:element name="DownwardCompatibleMasks" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="DownwardCompatibleMask" type="DownwardCompatibleMask" minOccurs="0" maxOccurs="unbounded" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="MaskEntries" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="MaskEntry" type="MaskEntry" minOccurs="0" maxOccurs="unbounded" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="HawkConfigurationData" type="HawkConfigurationData" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="MaskVersion" type="xs:integer" use="required" />
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="ManagementModel" type="xs:string" />
        <xs:attribute name="MediumTypeRefId" type="xs:string" />
        <xs:attribute name="OtherMediumTypeRefId" type="xs:string" />
        <xs:attribute name="MgmtDescriptor01" type="xs:hexBinary" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="DownwardCompatibleMask">
    <xs:attribute name="RefId" type="xs:string" use="required" />
  </xs:complexType>

  <xs:complexType name="MaskEntry">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="Address" type="xs:integer" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="HawkConfigurationData">
    <xs:sequence>
      <xs:element name="Features" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Feature" type="Feature" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Resources" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Resource" type="Resource" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Procedures" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Procedure" type="Procedure" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="MemorySegments" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="MemorySegment" type="MemorySegment" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="InterfaceObjects" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="InterfaceObject" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="LegacyVersion" type="xs:integer" />
    <xs:attribute name="Ets3SystemPlugin" type="xs:string" />
  </xs:complexType>

  <xs:complexType name="Feature">
    <xs:attribute name="Name" type="xs:string" use="required" />
    <xs:attribute name="Value" type="xs:integer" />
  </xs:complexType>

  <xs:complexType name="Resource">
    <xs:all>
      <xs:element name="ResourceType" type="ResourceType" minOccurs="1" maxOccurs="1" />
      <xs:element name="AccessRights" type="AccessRights" minOccurs="1" maxOccurs="1" />
      <xs:element name="Location" type="ResourceLocation" minOccurs="0" maxOccurs="1" />
      <xs:element name="ImgLocation" type="ResourceImgLocation" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="Name" type="xs:string" use="required" />
    <xs:attribute name="Access" type="xs:string" />
    <xs:attribute name="MgmtStyle" type="xs:string" />
  </xs:complexType>

  <xs:complexType name="ResourceType">
    <xs:attribute name="Length" type="xs:integer" use="required" />
    <xs:attribute name="Flavour" type="xs:string" />
  </xs:complexType>

  <xs:complexType name="AccessRights">
    <xs:attribute name="Read" type="AccessRight" use="required" />
    <xs:attribute name="Write" type="AccessRight" use="required" />
  </xs:complexType>

  <xs:simpleType name="AccessRight">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Configuration" />
      <xs:enumeration value="Manufacturer" />
      <xs:enumeration value="None" />
      <xs:enumeration value="Runtime" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="AbstractResourceLocation" abstract="true">
    <xs:attribute name="AddressSpace" type="AddressSpace" use="required" />
    <xs:attribute name="PtrResource" type="xs:string" />
    <xs:attribute name="InterfaceObjectRef" type="xs:integer" />
    <xs:attribute name="PropertyID" type="xs:integer" />
    <xs:attribute name="StartAddress" type="xs:integer" />
  </xs:complexType>

  <xs:simpleType name="AddressSpace">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ADC" />
      <xs:enumeration value="Constant" />
      <xs:enumeration value="LcFilter" />
      <xs:enumeration value="LcFilterMemory" />
      <xs:enumeration value="LcSlave" />
      <xs:enumeration value="LcSlaveMemory" />
      <xs:enumeration value="None" />
      <xs:enumeration value="Pointer" />
      <xs:enumeration value="Property" />
      <xs:enumeration value="RelativeMemory" />
      <xs:enumeration value="StandardMemory" />
      <xs:enumeration value="SystemProperty" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="ResourceLocation">
    <xs:complexContent>
      <xs:extension base="AbstractResourceLocation">
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ResourceImgLocation">
    <xs:complexContent>
      <xs:extension base="AbstractResourceLocation">
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="Procedure">
    <xs:complexContent>
      <xs:extension base="AbstractProcedure">
        <xs:attribute name="ProcedureType" type="ProcedureType" use="required" />
        <xs:attribute name="ProcedureSubType" type="xs:string" use="required" />
        <xs:attribute name="Access" type="xs:string" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:simpleType name="ProcedureType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Load" />
      <xs:enumeration value="Unload" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="MemorySegment">
    <xs:all>
      <xs:element name="Location" type="MemorySegmentLocation" minOccurs="0" maxOccurs="1" />
      <xs:element name="AccessRights" type="AccessRights" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="MemoryType" type="MemoryType" use="required" />
    <xs:attribute name="Length" type="xs:integer" use="required" />
    <xs:attribute name="Optional" type="xs:boolean" />
  </xs:complexType>
  
  <xs:complexType name="MemorySegmentLocation">
    <xs:attribute name="AddressSpace" type="AddressSpace" use="required" />
    <xs:attribute name="StartAddress" type="xs:integer" use="required" />
  </xs:complexType>

  <xs:simpleType name="MemoryType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="EEPROM" />
    </xs:restriction>
  </xs:simpleType>
  
  <xs:complexType name="InterfaceObject">
    <xs:sequence>
      <xs:element name="Property" type="InterfaceObjectProperty" minOccurs="0" maxOccurs="unbounded" />
    </xs:sequence>
    <xs:attribute name="ObjectType" type="xs:integer" use="required" />
  </xs:complexType>
  
  <xs:complexType name="InterfaceObjectProperty">
    <xs:attribute name="PropertyID" type="xs:integer" use="required" />
    <xs:attribute name="PropertyDataType" type="xs:string" use="required" />
  </xs:complexType>

  <xs:complexType name="ManufacturerDetails">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:sequence>
          <xs:element name="PublicKeys" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="PublicKey" type="PublicKey" minOccurs="0" maxOccurs="unbounded" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="KnxManufacturerId" type="xs:integer" />
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="DefaultLanguage" type="xs:string" />
        <xs:attribute name="ImportRestriction" type="xs:string" />
        <xs:attribute name="ImportGroup" type="xs:string" />
        <xs:attribute name="CompatibilityGroup" type="xs:integer" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="PublicKey">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:sequence>
          <xs:element name="RSAKeyValue" type="RSAKeyValue" minOccurs="0" maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="Number" type="xs:integer" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="RSAKeyValue">
    <xs:all>
      <xs:element name="Modulus" type="xs:base64Binary" minOccurs="1" maxOccurs="1" />
      <xs:element name="Exponent" type="xs:base64Binary" minOccurs="1" maxOccurs="1" />
    </xs:all>
  </xs:complexType>


  <!--
         Manufacturer Data 
    -->

  <xs:complexType name="ManufacturerData">
    <xs:sequence>
      <xs:element name="Manufacturer" type="Manufacturer" minOccurs="0" maxOccurs="1" />
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="Manufacturer">
    <xs:all>
      <xs:element name="Catalog" type="Catalog" minOccurs="0" maxOccurs="1" />
      <xs:element name="Hardware" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Hardware" type="Hardware" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="ApplicationPrograms" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ApplicationProgram" type="ApplicationProgram" minOccurs="0" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Languages" type="Languages" minOccurs="0" maxOccurs="1" />
    </xs:all>
    <xs:attribute name="RefId" type="xs:string" />
  </xs:complexType>

  <xs:element name="ParameterOrUnion" abstract="true" />
  <xs:element name="Parameter" type="Parameter" substitutionGroup="ParameterOrUnion" />
  <xs:element name="Union" type="Union" substitutionGroup="ParameterOrUnion" />

  <xs:complexType name="ApplicationProgram">
    <xs:complexContent>
      <xs:extension base="Identified">
        <xs:sequence>
          <xs:element name="Static" type="StaticPart" minOccurs="1" maxOccurs="1" />
          <xs:element name="Dynamic" type="DynamicPart" minOccurs="0" maxOccurs="1" />
          <xs:element name="RegistrationInfo" type="RegistrationInfo" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
        <xs:attribute name="Name" type="xs:string" use="required" />
        <xs:attribute name="VisibleDescription" type="xs:string" />
        <xs:attribute name="ApplicationNumber" type="xs:string" />
        <xs:attribute name="ApplicationVersion" type="xs:string" />
        <xs:attribute name="ProgramType" type="xs:string" />
        <xs:attribute name="MaskVersion" type="xs:string" />
